{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "/api-docs/$1"
    },
    {
      "src": "/swagger-ui/(.*)",
      "dest": "/swagger-ui/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api-docs",
      "dest": "/api-docs/index.html"
    },
    {
      "src": "/swagger-ui",
      "dest": "/swagger-ui/index.html"
    }
  ]
}
